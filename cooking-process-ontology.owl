###  http://www.semanticweb.org/at/ac/tuwien/ifs/188.387/ontologies/cooking-process-ontology#after

:after rdf:type owl:IrreflexiveProperty ,
                owl:ObjectProperty ;
       
       owl:inverseOf :before ;
       
       rdfs:range [ rdf:type owl:Restriction ;
                    owl:onProperty :after ;
                    owl:allValuesFrom :RecipeStep
                  ] ;
       
       rdfs:domain [ rdf:type owl:Restriction ;
                     owl:onProperty :after ;
                     owl:allValuesFrom :RecipeStep
                   ] .



###  http://www.semanticweb.org/at/ac/tuwien/ifs/188.387/ontologies/cooking-process-ontology#before

:before rdf:type owl:IrreflexiveProperty ,
                 owl:ObjectProperty ;
        
        rdfs:range [ rdf:type owl:Restriction ;
                     owl:onProperty :before ;
                     owl:allValuesFrom :RecipeStep
                   ] ;
        
        rdfs:domain [ rdf:type owl:Restriction ;
                      owl:onProperty :before ;
                      owl:allValuesFrom :RecipeStep
                    ] .



###  http://www.semanticweb.org/at/ac/tuwien/ifs/188.387/ontologies/cooking-process-ontology#hasIngredient

:hasIngredient rdf:type owl:ObjectProperty ;
               
