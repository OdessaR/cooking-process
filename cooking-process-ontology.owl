                 rdfs:domain :SliceMeat ;
                 
                 rdfs:subPropertyOf :start .



###  http://www.semanticweb.org/at/ac/tuwien/ifs/188.387/ontologies/cooking-process-ontology#usesEquipment

:usesEquipment rdf:type owl:ObjectProperty ;
               
               rdfs:domain [ rdf:type owl:Restriction ;
                             owl:onProperty :usesEquipment ;
                             owl:someValuesFrom :RecipeStep
                           ] ;
               
               rdfs:range [ rdf:type owl:Restriction ;
                            owl:onProperty :usesEquipment ;
                            owl:allValuesFrom :Equipment
                          ] .





