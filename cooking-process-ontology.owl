###  http://www.semanticweb.org/at/ac/tuwien/ifs/188.387/ontologies/cooking-process-ontology#start

:start rdf:type owl:FunctionalProperty ,
                owl:ObjectProperty ,
                owl:ReflexiveProperty .



###  http://www.semanticweb.org/at/ac/tuwien/ifs/188.387/ontologies/cooking-process-ontology#start_CookPotatoes

:start_CookPotatoes rdf:type owl:ObjectProperty ;
                    
                    rdfs:domain :CookPotatoes ;
                    
                    rdfs:range :RecipeStep ;
                    
                    rdfs:subPropertyOf :start .



###  http://www.semanticweb.org/at/ac/tuwien/ifs/188.387/ontologies/cooking-process-ontology#start_PrepareEggs

:start_PrepareEggs rdf:type owl:ObjectProperty ;
                   
                   rdfs:domain :PrepareEggs ;
                   
                   rdfs:range :RecipeStep ;
                   
                   rdfs:subPropertyOf :start .



###  http://www.semanticweb.org/at/ac/tuwien/ifs/188.387/ontologies/cooking-process-ontology#start_PrepareVegetables

:start_PrepareVegetables rdf:type owl:ObjectProperty ;
                         
                         rdfs:domain :PrepareVegetables ;
                         
                         rdfs:range :RecipeStep ;
                         
                         rdfs:subPropertyOf :start .



###  http://www.semanticweb.org/at/ac/tuwien/ifs/188.387/ontologies/cooking-process-ontology#start_SliceMeat

:start_SliceMeat rdf:type owl:ObjectProperty ;
                 
