#################################################################


###  http://www.semanticweb.org/at/ac/tuwien/ifs/188.387/ontologies/cooking-process-ontology#after

:after rdf:type owl:IrreflexiveProperty ,
                owl:ObjectProperty ;
       
       owl:inverseOf :before ;
       
       rdfs:range [ rdf:type owl:Restriction ;
                    owl:onProperty :after ;
                    owl:allValuesFrom :RecipeStep
                  ] ;
       
       rdfs:domain [ rdf:type owl:Restriction ;
                     owl:onProperty :after ;
                     owl:allValuesFrom :RecipeStep
                   ] .



###  http://www.semanticweb.org/at/ac/tuwien/ifs/188.387/ontologies/cooking-process-ontology#before

:before rdf:type owl:AsymmetricProperty ,
                 owl:FunctionalProperty ,
                 owl:IrreflexiveProperty ,
                 owl:ObjectProperty ;
        
        rdfs:domain [ rdf:type owl:Restriction ;
                      owl:onProperty :before ;
                      owl:allValuesFrom :RecipeStep
                    ] ;
        
        rdfs:range [ rdf:type owl:Restriction ;
                     owl:onProperty :before ;
                     owl:allValuesFrom :RecipeStep
                   ] .



###  http://www.semanticweb.org/at/ac/tuwien/ifs/188.387/ontologies/cooking-process-ontology#hasIngredient

:hasIngredient rdf:type owl:ObjectProperty ;
               
               rdfs:range [ rdf:type owl:Restriction ;
                            owl:onProperty :hasIngredient ;
                            owl:allValuesFrom :Ingredient
                          ] ;
               
               rdfs:domain [ rdf:type owl:Restriction ;
                             owl:onProperty :hasIngredient ;
                             owl:someValuesFrom :RecipeStep
                           ] .



###  http://www.semanticweb.org/at/ac/tuwien/ifs/188.387/ontologies/cooking-process-ontology#next_Cook

:next_Cook rdf:type owl:ObjectProperty ;
           
           rdfs:range :Cook ;
           
           rdfs:subPropertyOf :before .



###  http://www.semanticweb.org/at/ac/tuwien/ifs/188.387/ontologies/cooking-process-ontology#next_CookPasta

:next_CookPasta rdf:type owl:ObjectProperty ;
                
                rdfs:range :CookPasta ;
                
                rdfs:subPropertyOf :next_Cook .



###  http://www.semanticweb.org/at/ac/tuwien/ifs/188.387/ontologies/cooking-process-ontology#next_Flavor

:next_Flavor rdf:type owl:ObjectProperty ;
             
             rdfs:range :Flavor ;
             
             rdfs:subPropertyOf :before .



###  http://www.semanticweb.org/at/ac/tuwien/ifs/188.387/ontologies/cooking-process-ontology#next_Fry

:next_Fry rdf:type owl:ObjectProperty ;
          
          rdfs:range :Fry ;
          
          rdfs:subPropertyOf :before .



###  http://www.semanticweb.org/at/ac/tuwien/ifs/188.387/ontologies/cooking-process-ontology#next_MakeBreading

:next_MakeBreading rdf:type owl:ObjectProperty ;
                   
                   rdfs:range :MakeBreading ;
                   
                   rdfs:domain :RecipeStep ;
                   
                   rdfs:subPropertyOf :before .



###  http://www.semanticweb.org/at/ac/tuwien/ifs/188.387/ontologies/cooking-process-ontology#next_PrepareVegetables

:next_PrepareVegetables rdf:type owl:ObjectProperty ;
                        
                        rdfs:range :PrepareVegetables ;
                        
                        rdfs:domain :RecipeStep ;
                        
