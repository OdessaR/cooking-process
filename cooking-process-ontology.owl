###  http://www.semanticweb.org/at/ac/tuwien/ifs/188.387/ontologies/cooking-process-ontology#Cook

:Cook rdf:type owl:Class ;
      
      owl:equivalentClass [ rdf:type owl:Class ;
                            owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                   owl:onProperty :hasIngredient ;
                                                   owl:someValuesFrom :Ingredient
                                                 ]
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty :usesEquipment ;
                                                   owl:onClass :Cooker ;
                                                   owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger
                                                 ]
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty :usesEquipment ;
                                                   owl:onClass :Cookware ;
                                                   owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger
                                                 ]
                                               )
                          ] ;
      
      rdfs:subClassOf :RecipeStep ;
      
      rdfs:comment "Put ingredients into cookware and heat."^^rdfs:Literal .



###  http://www.semanticweb.org/at/ac/tuwien/ifs/188.387/ontologies/cooking-process-ontology#Cooker

:Cooker rdf:type owl:Class ;
        
        rdfs:subClassOf :Equipment .



###  http://www.semanticweb.org/at/ac/tuwien/ifs/188.387/ontologies/cooking-process-ontology#Cookware

