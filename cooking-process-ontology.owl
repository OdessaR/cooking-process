                                                            )
                                       ] ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty :before ;
                                         owl:someValuesFrom :Cook
                                       ] ;
                   
                   rdfs:subClassOf :RecipeStep ;
                   
                   rdfs:comment "Clean vegetables and cut into pieces with a knife."^^rdfs:Literal .



